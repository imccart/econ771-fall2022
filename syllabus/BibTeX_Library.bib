	year = {2015},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/1475-6773.12304},
	keywords = {health care, hospitals},
	pages = {2037--2047},
	file = {Full Text PDF:/home/imccart/Zotero/storage/59TM247J/Neprash et al. - 2015 - Measuring Prices in Health Care Markets Using Comm.pdf:application/pdf;Snapshot:/home/imccart/Zotero/storage/8ZENAGPU/1475-6773.html:text/html;Snapshot:/home/imccart/Zotero/storage/42J4EFXK/1475-6773.html:text/html},
