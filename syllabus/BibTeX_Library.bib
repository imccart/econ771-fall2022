@article{ketcham2016,
	title = {Choice {Inconsistencies} among the {Elderly}: {Evidence} from {Plan} {Choice} in the {Medicare} {Part} {D} {Program}: {Comment}},
	volume = {106},
	issn = {0002-8282},
	shorttitle = {Choice {Inconsistencies} among the {Elderly}},
	url = {http://www.aeaweb.org/articles?id=10.1257/aer.20131048},
	doi = {10.1257/aer.20131048},
	abstract = {Consumers' enrollment decisions in Medicare Part D can be explained by Abaluck and Gruber's (2011) model of utility maximization with psychological biases or by a neoclassical version of their model that precludes such biases. We evaluate these competing hypotheses by applying nonparametric tests of utility maximization and model validation tests to administrative data. We find that 79 percent of enrollment decisions from 2006 to 2010 satisfied basic axioms of consumer theory under the assumption of full information. The validation tests provide evidence against widespread psychological biases. In particular, we find that precluding psychological biases improves the structural model's out-of-sample predictions for consumer behavior.},
	language = {en},
	number = {12},
	urldate = {2022-01-07},
	journal = {American Economic Review},
	author = {Ketcham, Jonathan D. and Kuminoff, Nicolai V. and Powers, Christopher A.},
	month = dec,
	year = {2016},
	pages = {3932--3961},
	file = {Full Text PDF:/home/imccart/Zotero/storage/3ZWESNDD/Ketcham et al. - 2016 - Choice Inconsistencies among the Elderly Evidence.pdf:application/pdf;Snapshot:/home/imccart/Zotero/storage/LM57YNMY/articles.html:text/html;Snapshot:/home/imccart/Zotero/storage/7ZDV3D6D/articles.html:text/html},
}

