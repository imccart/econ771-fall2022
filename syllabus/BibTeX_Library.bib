	keywords = {wages},
	pages = {251--333},
	file = {Abowd Kramarz and Margolis 1999.pdf:/home/imccart/Zotero/storage/5JWL77ZG/Abowd Kramarz and Margolis 1999.pdf:application/pdf;Full Text PDF:/home/imccart/Zotero/storage/A2A2MJDL/Abowd et al. - 1999 - High Wage Workers and High Wage Firms.pdf:application/pdf;Snapshot:/home/imccart/Zotero/storage/TRZDSHRR/1468-0262.html:text/html;Snapshot:/home/imccart/Zotero/storage/K52YGINY/1468-0262.html:text/html},
